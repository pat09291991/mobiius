{
  "plugins": ["unused-imports", "@typescript-eslint"],
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      { "argsIgnorePattern": "^_" }
    ],
    "@typescript-eslint/no-explicit-any": "warn",
    "unused-imports/no-unused-imports": "warn",
    "no-console": "warn",
    "no-var": "warn",
    "prefer-const": "warn"
  }
}
